{
    "name": "alexperez/symfony-websocket-command-executor",
    "description": "Execute Symfony commands through WebSocket with interactive support",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AlexPerez\\SymfonyWebsocketCommandExecutor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\AlexPerez\\SymfonyWebsocketCommandExecutor\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alex Perez",
            "email": "alex@example.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "cboden/ratchet": "^0.4.4",
        "symfony/console": "^7.0",
        "symfony/process": "^7.0",
        "symfony/dependency-injection": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "minimum-stability": "stable"
}
