{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "beta",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-grpc": "*",
    "ext-iconv": "*",
    "ext-mongodb": "*",
    "ext-opentelemetry": "*",
    "ext-redis": "*",
    "doctrine/mongodb-odm-bundle": "*",
    "friendsofopentelemetry/opentelemetry-bundle": "^1.0@beta",
    "league/tactician": "*",
    "league/tactician-bundle": "*",
    "mcaskill/composer-exclude-files": "^4.0",
    "open-telemetry/transport-grpc": "^1.1",
    "php-http/httplug": "*",
    "phpdocumentor/reflection-docblock": "^5.6",
    "phpstan/phpdoc-parser": "^2.1",
    "predis/predis": "^2.3",
    "ramsey/uuid": "^4.7",
    "symfony/browser-kit": "7.2.*",
    "symfony/console": "7.2.*",
    "symfony/dotenv": "7.2.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "7.2.*",
    "symfony/http-client": "*",
    "symfony/monolog-bundle": "^3.10",
    "symfony/property-access": "7.2.*",
    "symfony/property-info": "7.2.*",
    "symfony/runtime": "7.2.*",
    "symfony/serializer": "7.2.*",
    "symfony/yaml": "7.2.*"
  },
  "config": {
    "allow-plugins": {
      "mcaskill/composer-exclude-files": true,
      "php-http/discovery": false,
      "symfony/flex": true,
      "symfony/runtime": true,
      "tbachert/spi": false
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "AlexPerez\\CoffeeMachine\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlexPerez\\CoffeeMachine\\Tests\\Unit\\": "tests/Unit",
      "AlexPerez\\CoffeeMachine\\Tests\\Integration\\": "tests/Integration"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.2.*"
    },
    "exclude-from-files": [
      "open-telemetry/*"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "rector/rector": "^2.0"
  }
}
