{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-mongodb": "*",
    "ext-redis": "*",
    "alexperez/symfony-websocket-command-executor": "*",
    "doctrine/mongodb-odm-bundle": ">=5.3",
    "league/tactician": ">=1.1",
    "league/tactician-bundle": ">=1.5.2",
    "phpdocumentor/reflection-docblock": "^5.6.2",
    "phpstan/phpdoc-parser": "^2.1",
    "predis/predis": "^2.4",
    "ramsey/uuid": "^4.7.6",
    "symfony/browser-kit": "7.2.*",
    "symfony/console": "7.2.*",
    "symfony/dotenv": "7.2.*",
    "symfony/flex": "^2.5",
    "symfony/framework-bundle": "7.2.*",
    "symfony/property-access": "7.2.*",
    "symfony/property-info": "7.2.*",
    "symfony/runtime": "7.2.*",
    "symfony/serializer": "7.2.*",
    "symfony/yaml": "7.2.*"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "path",
      "url": "./packages/symfony-websocket-command-executor"
    }
  ],
  "autoload": {
    "psr-4": {
      "AlexPerez\\CoffeeMachine\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlexPerez\\CoffeeMachine\\Tests\\Unit\\": "tests/Unit",
      "AlexPerez\\CoffeeMachine\\Tests\\Integration\\": "tests/Integration"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.2.*"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^12.1.3",
    "rector/rector": "^2.0.14"
  }
}
